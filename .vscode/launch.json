{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Rust (MinGW64)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/x86_64-pc-windows-gnu/debug/snow_frontend_egui.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "rust: cargo build (MinGW64)",
            "stopAtEntry": false,
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\msys64\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Debug Rust (MSVC)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/x86_64-pc-windows-msvc/debug/snow_frontend_egui.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "rust: cargo build (MSVC)",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        }
    ]
}